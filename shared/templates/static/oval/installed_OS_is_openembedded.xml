<def-group>
  <definition class="inventory" id="installed_OS_is_openembedded" version="1">
    <metadata>
      <title>OpenEmbedded</title>
      <affected family="unix">
        <platform>multi_platform_all</platform>
      </affected>
      <reference ref_id="cpe:/o:openembedded:nodistro"
      source="CPE" />
      <description>The operating system installed on the system is
      OpenEmbeddd </description>
    </metadata>
    <criteria>
      <criterion comment="Installed operating system is part of the unix family" test_ref="test_unix_openembedded" />
      <criterion comment="OpenEmbedded is installed" test_ref="test_openembedded" />
    </criteria>
  </definition>

  <ind:family_test check="all" check_existence="at_least_one_exists" comment="installed OS part of unix family" id="test_unix_openembedded" version="1">
    <ind:object object_ref="obj_unix_openembedded" />
    <ind:state state_ref="state_unix_openembedded" />
  </ind:family_test>

  <ind:family_state id="state_unix_openembedded" version="1">
    <ind:family>unix</ind:family>
  </ind:family_state>

  <ind:family_object id="obj_unix_openembedded" version="1" />

  <file_test xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5#unix" check_existence="at_least_one_exists" version="1" id="test_openembedded" check="all" comment="Test presence of /etc/os-release.">
      <object object_ref="obj_test_openembedded" />
  </file_test>

  <file_object xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5#unix" version="1" id="obj_test_openembedded" >
      <filepath>/etc/os-release</filepath>
  </file_object>

</def-group>
